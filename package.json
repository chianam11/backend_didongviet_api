{
  "name": "be",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "migrate": "npx sequelize-cli db:migrate --env production",
    "undo-migrate": "npx sequelize-cli db:migrate:undo:all --env production",
    "seed": "npx sequelize-cli db:seed:all --env production",
    "undo-seed": "npx sequelize-cli db:seed:undo:all --env production",
    "seed:reset": "npm run undo-seed && npm run seed",
    "production": "npm run build && npm run migrate && npm run seed && cross-env BUILD_MODE=production node ./bin/www",
    "dev": "cross-env BUILD_MODE=dev nodemon ./bin/www"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.1",
    "html-entities": "^2.5.2",
    "http-errors": "~1.6.3",
    "jsdom": "^26.0.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "postcss": "^8.4.49",
    "sequelize": "^6.37.5",
    "sharp": "^0.34.2",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.17",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.4",
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "postcss-cli": "^11.0.0",
    "sequelize-cli": "^6.6.2"
  }
}
